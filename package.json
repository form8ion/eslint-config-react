{
  "name": "@form8ion/eslint-config-react",
  "description": "Shareable config for ESLint for React components",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/eslint-config-react",
  "bugs": "https://github.com/form8ion/eslint-config-react/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-react",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "7.23.10",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "devDependencies": {
    "@form8ion/babel-preset": "1.6.122",
    "@form8ion/commitlint-config": "1.0.71",
    "@form8ion/eslint-config": "7.0.9",
    "@form8ion/remark-lint-preset": "6.0.2",
    "ban-sensitive-files": "1.10.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.0.11",
    "npm-run-all2": "6.1.2",
    "publint": "0.2.7",
    "remark-cli": "12.0.0"
  }
}
