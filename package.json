{
  "name": "@form8ion/eslint-config-react",
  "description": "Shareable config for ESLint for React components",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/eslint-config-react",
  "bugs": "https://github.com/form8ion/eslint-config-react/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-react",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "7.22.10",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "devDependencies": {
    "@form8ion/babel-preset": "1.6.108",
    "@form8ion/commitlint-config": "1.0.55",
    "@form8ion/eslint-config": "6.0.1",
    "@form8ion/remark-lint-preset": "6.0.0",
    "ban-sensitive-files": "1.9.19",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "npm-run-all": "4.1.5",
    "publint": "0.2.0",
    "remark-cli": "11.0.0"
  }
}
