{
  "name": "@form8ion/eslint-config-react",
  "description": "Shareable config for ESLint for React components",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "form8ion/eslint-config-react",
  "bugs": "https://github.com/form8ion/eslint-config-react/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-react",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.12",
    "@form8ion/eslint-config": "1.6.28",
    "@form8ion/remark-lint-preset": "1.0.9",
    "ban-sensitive-files": "1.9.9",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "remark-cli": "8.0.1",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "babel-eslint": "10.1.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0"
  }
}
